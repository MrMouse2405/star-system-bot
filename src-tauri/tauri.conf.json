{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "Star System Bot",
  "version": "0.1.0",
  "identifier": "com.star-system-bot.dev",
  "build": {
    "frontendDist": "../build",
    "devUrl": "http://localhost:1420",
    "beforeDevCommand": "bun run dev",
    "beforeBuildCommand": "bun run build"
  },
  "app": {
    "windows": [
      {
        "title": "Star System Bot",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": [
      "appimage"
    ],
    "icon": [
      "icons/150x150.png",
      "icons/310x310.png"
    ],
    "resources": {
      "model/": "model/"
    }
  }
}