{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "Star System Bot",
  "version": "0.1.0",
  "identifier": "com.tauri.dev",
  "build": {
    "frontendDist": "../public",
    "devUrl": "http://localhost:1420",
    "beforeDevCommand": "pnpm dev",
    "beforeBuildCommand": "pnpm build"
  },
  "app": {
    "windows": [
      {
        "title": "Star System Bot",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/150x150.png",
      "icons/310x310.png"
    ],
    "resources": {
      "m2m100_model/": "m2m100_model/"
    }
  }
}